{
  "name": "list-estate",
  "version": "1.0.0",
  "description": "ListEstate - searching properties made easy!",
  "scripts": {
    "tsc": "tsc -p app",
    "tsc:w": "tsc -p app -w",
    "lite": "lite-server",
    "webpack": "node_modules/.bin/webpack --progress --colors",
    "webpack:w": "node_modules/.bin/webpack --progress --colors --watch",
    "liveserver": "live-server --entry-file=public/index.html",
    "start": "concurrent \"npm run tsc:w\" \"npm run webpack:w\" \"npm run liveserver\" "
  },
  "repository": {
    "type": "git"
  },
  "author": "sureshkismu@gmail.com",
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.0",
    "angular2-google-maps": "^0.5.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "systemjs": "0.19.6",
    "webpack": "^1.12.9",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "lite-server": "^1.3.1",
    "typescript": "^1.7.3"
  }
}
